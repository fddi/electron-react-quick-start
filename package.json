{
  "name": "electron-react-quick-start",
  "version": "0.1.0",
  "main": "public/main.js",
  "homepage": ".",
  "dependencies": {
    "antd": "3.10.7",
    "ffi": "git+https://github.com/node-ffi/node-ffi.git",
    "moment": "^2.19.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.9.1",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "electron": "^3.0.8",
    "electron-builder": "^20.28.4",
    "electron-is-dev": "^0.3.0",
    "electron-rebuild": "^1.8.2",
    "react-app-rewire-less": "^2.1.3",
    "react-app-rewired": "^1.6.2",
    "wait-on": "^3.0.1"
  },
  "scripts": {
    "web": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "start": "concurrently \"cross-env BROWSER=none react-app-rewired start\" \"wait-on http://localhost:3000 && electron .\"",
    "dist": "npm run-script build && electron-builder --win --ia32",
    "rebuild": "cd ./node_modules/.bin && electron-rebuild --force --module-dir=../../"
  }
}
